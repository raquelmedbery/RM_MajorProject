<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FileChoose">
   <children>
      <Label layoutX="64.0" layoutY="66.0" text="Choose Product File" />
      <Button fx:id="chooseFileBtnOne" layoutX="71.0" layoutY="125.0" mnemonicParsing="false" onAction="#fileChooseOne" text="Select One File" />
      <ListView fx:id="fileList" layoutX="260.0" layoutY="66.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="submitFileBtn" disable="true" layoutX="98.0" layoutY="185.0" mnemonicParsing="false" onAction="#getNewScene" text="Submit File" />
   </children>
</AnchorPane>
